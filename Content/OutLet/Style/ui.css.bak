	background-repeat: no-repeat;
	border-right:1px solid #ddd;
}

.sub-menu02 li:hover a { background: #ddd; }

.sub-menu02 li:last-child a { border: none; }

.sub-menu02 li > a span {
	color: #797979;
	text-shadow: 1px 1px 0px rgba(255,255,255, .2);
	background: #f7f7f7;
	border: 1px solid #d7d7d7;

	-webkit-box-shadow: none;
	-moz-box-shadow: none;
	box-shadow: none;
}


/* Functionality */

.xstock li > .sub-menu02 {
	display: none;
}

.xstock li:target > .sub-menu02 {
	display: block;
}
 
