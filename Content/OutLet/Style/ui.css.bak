.close {
  float: right;
  font-size: 21px;
  font-weight: bold;
  line-height: 1;
  color: #000;
  text-shadow: 0 1px 0 #fff;
  filter: alpha(opacity=50);
  opacity: .5;
}
.close:hover,
.close:focus {
  cursor: pointer;
  filter: alpha(opacity=90);
  opacity: .9;
}
button.close {
  -webkit-appearance: none;
  padding: 0;
  cursor: pointer;
  background: transparent;
  border: 0;
}



.alert {
	padding: 25px 15px 10px 140px;
	margin-bottom: 20px;
	border: 2px solid transparent;
	border-radius: 8px;
	width: 50%;
	position: absolute;
	top: 100px;
	left: 18%;
	z-index: 999;
	font-size: 28px;
	line-height: 35px;
	background-image: url(../img/icon_alert.png);
	background-repeat: no-repeat;
	background-position: 20px 20px;
	height:90px;
}

.alert-dismissable,
.alert-dismissible {
  padding-right: 35px;
}
.alert-dismissable .close,
.alert-dismissible .close {
  position: relative;
  top: -20px;
  right: -28px;
  color: inherit;
}
.alert-danger {
  color: #fff;
  background-color: #f00;
  border-color: #e00;
}

.alert-danger .alert-link {
  color: #843534;
}


